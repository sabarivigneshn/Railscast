<h2>New EpisodeType</h2>
<div class="container">
<%= form_tag("/episode_type",method: "post") do %>
    <p class=""form-group>
        <%= label_tag("name:")%>
        <%= text_field_tag "name"%>
    </p>
    <p class="form-group">
      <%= label_tag("notes:")%>
      <%= text_area_tag "notes"%>
    </p>
    <p class="form-group">
      <%= submit_tag "create" ,class: "btn btn-primary"%>
    </p>
<% end %>
    <%= link_to "Back", episode_type_index_path%>
</div>